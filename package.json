{
  "name": "@invoicedesk/sdk",
  "version": "1.1.0",
  "private": false,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/invoicedesk/sdk-js.git"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "prepare": "npm run build",
    "lint": "tsc",
    "release": "pnpm run build && changeset publish --access=public"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.2",
    "@types/node": "^20.4.2",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "axios": "^1.4.0"
  },
  "keywords": [
    "axios",
    "typescript",
    "openapi-client",
    "@invoicedesk/sdk"
  ]
}
